    SQL.Strings = (
      'SELECT idlance,'
      '    mareas_idmarea,'
      '    lpad(nro_lance,4,''0'') as nro_lance,'
      '    fecha,'
      '    date_format(hora, ''%h:%i'') AS hora,'
      '    latitud_ini,'
      '    longitud_ini,'
      '    latitud_fin,'
      '    longitud_fin,'
      '    rumbo,'
      '    profundidad,'
      '    cable_estribor,'
      '    cable_babor,'
      '    minutos_arrastre,'
      '    velocidad,'
      '    captura_estribor,'
      '    captura_babor,'
      '    comentarios,'
      '    temperatura_superficie,'
      '    temperatura_aire,'
      '    temperatura_fondo,'
      '    mar_beaufort,'
      '    nudos_viento,'
      '    direccion_viento,'
      '    largo_relinga_inferior,'
      '    mallero_copo_mm,'
      '    tipos_malla_tipo_malla'
      'FROM'
      '    vieira.lances'
      'WHERE'
      '  mareas_idmarea = :mareas_idmarea  AND'
      '(:__FILTRO__ IS NULL OR'
      ':__FILTRO__ = '''' OR'
      'lpad(nro_lance,4,''0'') LIKE CONCAT (''%'', :__FILTRO__, ''%'') OR'
      'comentarios LIKE CONCAT (''%'', :__FILTRO__, ''%'') OR'
      'latitud_ini LIKE CONCAT (''%'', :__FILTRO__, ''%'') OR'
      'longitud_fin LIKE CONCAT (''%'', :__FILTRO__, ''%'') OR'
      'minutos_arrastre LIKE CONCAT (''%'', :__FILTRO__, ''%'') OR'
      'DATE_FORMAT(fecha, ''%d/%m/%Y'') LIKE CONCAT (''%'', :__FILTRO__, ''%'') OR'
      'DATE_FORMAT(hora, ''%h:%i'') LIKE CONCAT (''%'', :__FILTRO__, ''%''))'
      'order by nro_lance'
    )
