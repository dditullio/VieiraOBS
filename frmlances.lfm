  object zqLances: TZQuery[4]
    Connection = dmGeneral.zcDB
    BeforeOpen = zqLancesBeforeOpen
    OnCalcFields = zqLancesCalcFields
    SQL.Strings = (
      'SELECT idlance,'
      '    idmarea,'
      '    nro_lance,'
      '    fecha,'
      '    hora,'
      '    grados_latitud_ini,'
      '    minutos_latitud_ini,'
