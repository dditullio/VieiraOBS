      '    cuadrante_longitud_fin,'
      '    latitud_ini,'
      '    longitud_ini,'
      '    latitud_fin,'
      '    longitud_fin,'
      '    latitud_ini_fmt,'
      '    longitud_ini_fmt,'
      '    latitud_fin_fmt,'
      '    longitud_fin_fmt,'
      '    rumbo,'
      '    profundidad,'
      '    cable_estribor,'
      '    cable_babor,'
      '    minutos_arrastre,'
      '    captura_estribor,'
      '    captura_babor,'
      '    velocidad,'
      '    rinde_total_E,'
      '    rinde_total_B,'
      '    rinde_comercial_E,'
      '    rinde_comercial_B,'
      '    comentarios,'
      '    temperatura_superficie,'
      '    temperatura_aire,'
      '    temperatura_fondo,'
      '    cod_estado_mar,'
      '    nudos_viento,'
      '    direccion_viento,'
      '    largo_relinga_inferior,'
      '    mallero_copo_mm,'
      '    tipo_malla'
      'FROM v_lances_simple'
      'WHERE idmarea=:idmarea'
      'AND (:fecha IS NULL'
      'OR :fecha = '''''
      'OR fecha=:fecha)'
      ' AND ('
      ':__FILTRO__ IS NULL OR'
      ':__FILTRO__ = '''''
      'OR date_format(fecha,''%d/%m/%Y'') LIKE CONCAT(''%'',:__FILTRO__,''%'')'
      'OR hora LIKE CONCAT(''%'',:__FILTRO__,''%'')'
      'OR comentarios LIKE CONCAT(''%'',:__FILTRO__,''%'')'
      'OR nro_lance LIKE CONCAT(''%'',:__FILTRO__,''%''))'
      'ORDER BY fecha asc , MAKETIME(HOUR(hora),MINUTE(hora),0) asc'
    )
