  object zqDetallleTallas: TZQuery[9]
    Connection = dmGeneral.zcDB
    BeforeOpen = zqDetallleTallasBeforeOpen
    SQL.Strings = (
      'SELECT dmt.*, IF(talla<35,255,IF(talla<55,33023,49152)) AS color'
      'FROM'
      '    vieira.detalle_muestras_talla dmt'
      'WHERE'
      '    idmuestras_talla = :idmuestras_talla'
      '        AND talla BETWEEN (SELECT '
      '            MIN(talla)'
      '        FROM'
      '            detalle_muestras_talla dmt1'
      '        WHERE'
      '            dmt1.idmuestras_talla = dmt.idmuestras_talla'
      '                AND nro_ejemplares > 0) AND (SELECT '
      '            MAX(talla)'
      '        FROM'
      '            detalle_muestras_talla dmt1'
      '        WHERE'
      '            dmt1.idmuestras_talla = dmt.idmuestras_talla'
      '                AND nro_ejemplares > 0)'
      'ORDER BY talla'
    )
